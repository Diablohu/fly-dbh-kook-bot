        "eslint-config-koot": "^4.0.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "koot-cli-kit": "^0.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "prettier-eslint": "^15.0.1",
        "react": "^18.2.0",
        "thread-loader": "^4.0.2",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "overrides": {},
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{xxxxxx}": [
            "tsc --noEmit"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write"
        ]
    }
