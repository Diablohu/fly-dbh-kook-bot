        "@babel/core": "^7.23.0",
        "@babel/preset-typescript": "^7.23.0",
        "@types/fs-extra": "^11.0.2",
        "@types/koa": "^2.13.9",
        "@types/koa-bodyparser": "^4.3.10",
        "@types/koa-router": "^7.4.5",
        "@types/listr": "^0.14.5",
        "@types/numeral": "^2.0.3",
        "@types/zen-observable": "^0.8.4",
        "babel-loader": "^9.1.3",
